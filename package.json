{
  "name": "velferd",
  "version": "1.0.0",
  "description": "Velferdsteknologi prototype",
  "main": "bundle.js",
  "author": "Direktoratet for e-helse",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ehelse/velferd.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/server.js",
    "start:mock": "SET NODE_ENV=mock && babel-node tools/server",
    "prestart": "babel-node tools/start-message.js",
    "lint": "eslint src --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "clean-dist": "npm run remove-dist && mkdir public",
    "remove-dist": "rimraf ./public",
    "prebuild": "npm-run-all clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "postbuild": "",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "chartist": "^0.9.8",
    "classnames": "^2.2.5",
    "react": "^15.0.2",
    "react-autosize-textarea": "^0.3.3",
    "react-chartist": "^0.10.2",
    "react-collapse": "^2.3.3",
    "react-dom": "^15.0.2",
    "react-height": "^2.1.1",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "scroll-to": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.3.0",
    "postcss": "^5.1.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "stylelint-webpack-plugin": "^0.3.0",
    "svg-sprite-loader": "0.0.26",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "whatwg-fetch": "^1.0.0"
  }
